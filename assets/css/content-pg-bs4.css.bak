  height: 100%;
  top: 0;
  left: 0; }

.flip-container.click-flip .front.side .card-img-overlay {
  opacity: 0; }

.card-img-overlay {
  position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0; }

.flip-container.click-flip .card-img-overlay .overlay-elements {
