  margin: 0;
  border: 2px solid;
  border-color: #f3590d;
  border-radius: 20px;
  padding: 8% 8% 9% 10%; }

/* bubblequote right double quotes */
.bubblequote h3:after {
  content: "";
  position: absolute;
  border: 2px solid #f3590d;
  border-radius: 0 50px 0 0;
  width: 16%;
  height: 13%;
  bottom: -13%;
  left: 13%;
  border-bottom: none;
  border-left: none;
  z-index: 3; }

.bubblequote h3:before {
  content: "";
  position: absolute;
  width: 25%;
  border: 6px solid #fff;
  bottom: -3px;
  left: 15%;
  z-index: 2; }

/* increase header size after 700px */
@media all and (min-width: 700px) {
  .bubblequote h3 {
    font-size: 21px;
    line-height: 1.3;
    letter-spacing: 0.1px; }

  .bubblequote h3:after {
    width: 16%;
    height: 15%;
    bottom: -15%;
    left: 15%; } }
/* bubblequote subheader */
.bubblequote h4 {
  position: relative;
  color: #777;
  font-size: 21px;
  font-weight: 400;
  line-height: 1.2;
  margin: 0;
  padding-top: 15px;
  z-index: 1;
  margin-left: 150px;
  padding-left: 12px; }

.bubblequote h4:first-letter {
  margin-left: -12px; }
  
  
  
